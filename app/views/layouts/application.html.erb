<!DOCTYPE html>
<html>
<head>
  <title><%= page_title %></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tag %>

</head>
  <body>
  <div class="container-fluid">
    <div class="row-fluid">
      <% if logged_in? %>
        <%= render 'layouts/dashboard_header' %>
        <% else %>
          <%= render 'layouts/header' %>
      <% end %>
    </div>
      <div class="row-fluid">
        <div class="span2 menu-system">
        <%= render "layouts/sitenav" unless current_user.nil? %> 
      </div>
      <div class="span10">
      <%= yield %>
      </div>
    </div>
    <div class="row-fluid">
    <%= render 'layouts/footer' %>
    </div>
    </div>
  </body>
</html>
