<!DOCTYPE html>
<html>
<head>
  <title><%= page_title %></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.1/jquery.min.js", 'application' %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="wrap">
  <%= render 'layouts/header' %>
  <div id="main">
      <%= render "layouts/sitenav" unless current_user.nil? %> 
      <%= yield %>
  </div>
</div>
  <%= render 'layouts/footer' %>
</body>
</html>
