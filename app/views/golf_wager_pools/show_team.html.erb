<div class="container_12">
  <div id="masters_team">
    <div class="grid_6">
      <ul>
        <% if(@team.nil?) %>
          <li>No team selected yet</li> 
        <% end %>
      </ul>
    </div>
    <div class="grid_6">
      <table>
        <tr>
          <th>Player</th>
        </tr>
        <% @tourney.masters_qualifiers.each do |mq| %>
          <tr>
            <td><%= mq.pga_player.name %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(function() { var TeamSelectionApp = new MastersTeamView; })
</script>

