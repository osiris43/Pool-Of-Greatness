<%= render 'shared/poolbanner' %>
<div class="container_12">
  <%= render 'confidence_pools/navigation' %>
  <%= render 'shared/flashpartial' %>
  <h3>Coming Soon!</h3>
  <%= label(:possibleCount, "#{@possibleCount+1} possible results remaining") %>
