    <% @fourths.each do |key, value| %>
      <tr>
        <td><%= key %></td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </table>
  <br />
  <%= label(:dals, "DAL") %>
  <table>
    <% @dals.each do |key, value| %>
      <tr>
        <td><%= key %></td>
        <td><%= value %></td>
